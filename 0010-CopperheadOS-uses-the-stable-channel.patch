From: Daniel Micay <danielmicay@gmail.com>
Date: Sun, 4 Dec 2016 22:30:01 -0500
Subject: [PATCH 10/22] CopperheadOS uses the stable channel

---
 components/version_info/channel_android.cc | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/components/version_info/channel_android.cc b/components/version_info/channel_android.cc
index 4e1cd4700d7c..416ed78cb048 100644
--- a/components/version_info/channel_android.cc
+++ b/components/version_info/channel_android.cc
@@ -18,7 +18,7 @@ Channel ChannelFromPackageName(const char* package_name) {
   if (!strcmp(package_name, "com.chrome.canary"))
     return Channel::CANARY;
 
-  return Channel::UNKNOWN;
+  return Channel::STABLE;
 }
 
 }  // namespace version_info
